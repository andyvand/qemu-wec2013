    pl041.def

TARGETLIBS= \
    $(_COMMONSDKROOT)\lib\$(_CPUDEPPATH)\ceddk.lib \
    $(_COMMONSDKROOT)\lib\$(_CPUDEPPATH)\coredll.lib

SOURCELIBS= \
    $(_COMMONOAKROOT)\lib\$(_CPUDEPPATH)\wavemdd.lib

