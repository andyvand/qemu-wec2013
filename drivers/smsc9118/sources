!IF "$(BSP_QEMU_LAN9118)" == ""
SKIPBUILD=1
!ENDIF

RELEASETYPE=PLATFORM
TARGETNAME=smsc9118
TARGETTYPE=DYNLINK
TARGETDEFNAME=smsc9118
DEFFILE=$(TARGETDEFNAME).def
WINCEREL=1

DLLENTRY=DllEntry

PLAT_SOURCES=platform.c util.S
CDEFINES=$(CDEFINES)

!IF "$(WINCEDEBUG)" == "debug"
CDEFINES=$(CDEFINES) -DSMSC_DBG -DDEBUG
!ENDIF

TARGETLIBS=	$(_COMMONSDKROOT)\lib\$(_CPUINDPATH)\coredll.lib \
		  	$(_COMMONOAKROOT)\lib\$(_CPUDEPPATH)\ceddk.lib \
		  	$(_COMMONSDKROOT)\lib\$(_CPUINDPATH)\ndis.lib \
		  	$(_COMMONSDKROOT)\lib\$(_CPUINDPATH)\winsock.lib \

SOURCES=		\
	lan9118.c	\
	os.c		\
	smsc9118.c	\
	$(PLAT_SOURCES)

POSTLINK_PASS_CMD=postlink.bat
PRELINK_PASS_CMD=prelink.bat
FILE_VIEW_ROOT_FOLDER= \
    prelink.bat \
    postlink.bat

